
@{
    ViewBag.Title = "Cadastro";
    Layout = "~/Views/Shared/Master.cshtml";
}

<h2>Cadastro</h2>
<form action="/Idioma/Store" method="post">
    <div class="form-group">
       
    </div>
    <div class="form-group">
        <label for="cadastro-idioma-campo-nome">Nome</label>
        <input type="text" id="cadastro-idioma-campo-nome" class="form-control" value="@ViewBag.Idioma.Nome" name="idioma.Nome" />

    </div>
    <div>
        <button type="submit" class="btn btn-success float-right">
            <i class="fa fa-save"></i>Cadastrar
        </button>
    </div>
</form>

@section script{
    <script scr="~/Scripts/Idioma/Cadastro/select2-cadastro-idioma.js"></script>
}










