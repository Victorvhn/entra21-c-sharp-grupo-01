
@{
    Layout = null;
}

<!DOCTYPE html>

<div class="modal fade" id="viagem-turista-modal-cadastro" tabindex="-1" role="dialog" style="z-index: 1050; display: none; " aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <h4 class="modal-title">Title</h4>
            <button type="button" class="close" data-dissmiss="modal" aria-label="Close">
                <span aria-hidden="true">X</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="row mb-md-3">
                <div class="col-md-6">
                    <label for="select-cadastro-viagem-turista-turista">Turista</label>
                    <select id="select-cadastro-viagem-turista-turista" name="viagemTurista.IdTurista" class="form-control"></select>
                </div>
                <div class="col-md-6">
                    <label for="select-cadastro-viagem-turista-viagem">Viagem</label>
                    <select id="select-cadastro-viagem-turista-viagem" name="viagemTurista.IdViagem" class="form-control"></select>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <label for="campo-cadastro-valor-viagem-turista">Valor</label>
                    <input class="form-control" name="viagemTurista.Valor" id="campo-cadastro-valor-viagem-turista" />
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default waves-effect" data-dissmiss="modal" id="botao-fechar-modal-cadastro">Fechar</button>
            <button type="button" class="btn btn-primary waves-effect waves-light" id="botao-salvar-modal-cadastrar">Cadastrar</button>
        </div>
    </div>
</div>

