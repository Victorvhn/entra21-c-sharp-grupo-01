                                    @Principal.Controllers.BaseController.CurrentIdiom.DisplayName
                                </a>
                                <ul class="show-notification">
                                    @foreach (System.Globalization.CultureInfo idiom in Principal.Controllers.BaseController.Idioms)
                                    {
                                        <li>
                                            <a href="/Language/Change?lang=@idiom.TwoLetterISOLanguageName">
                                                <img src="~/Content/assets/images/flags/@{@idiom.TwoLetterISOLanguageName}.svg" alt="">
                                                @idiom.DisplayName
                                            </a>
                                        </li>
                                    }
                                </ul>
                            </li>
                            <li class="header-notification">
                                <a href="#!" id="chat-botao" class="displayChatbox">
                                    <i class="ti-comments"></i>
                                    <span class="badge bg-c-green"></span>
                                </a>
                            </li>
                            <li class="user-profile header-notification">
                                <a href="#!">
                                    <img src="~/Content/assets/images/avatar-4.jpg" class="img-radius" alt="User-Profile-Image">
                                    <span>@ViewBag.UsuarioNome</span>
                                    <i class="ti-angle-down"></i>
                                </a>
                                <ul class="show-notification profile-notification">
                                    <li>
                                        <a href="#!">
                                            <i class="ti-settings"></i> @Principal.Resources.Resource.MeuPerfil
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/Login/LockScreen">
                                            <i class="ti-lock"></i> @Principal.Resources.Resource.TelaBloqueio
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/Login/Logout">
                                            <i class="ti-layout-sidebar-left"></i> @Principal.Resources.Resource.Sair
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                        <!-- search -->
                        <div id="morphsearch" class="morphsearch">
                            <form class="morphsearch-form">
                                <input class="morphsearch-input" type="search" placeholder="Search..." />
                                <button class="morphsearch-submit" type="submit">Search</button>
                            </form>
                            <div class="morphsearch-content">
                                <div class="dummy-column">
                                    <h2>People</h2>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-4.jpg" class="img-radius" alt="Sara Soueidan" />
                                        <h3>Sara Soueidan</h3>
                                    </a>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-2.jpg" class="img-radius" alt="Shaun Dona" />
                                        <h3>Shaun Dona</h3>
                                    </a>
                                </div>
                                <div class="dummy-column">
                                    <h2>Popular</h2>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-3.jpg" class="img-radius" alt="PagePreloadingEffect" />
                                        <h3>Page Preloading Effect</h3>
                                    </a>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-4.jpg" class="img-radius" alt="DraggableDualViewSlideshow" />
                                        <h3>Draggable Dual-View Slideshow</h3>
                                    </a>
                                </div>
                                <div class="dummy-column">
                                    <h2>Recent</h2>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-2.jpg" class="img-radius" alt="TooltipStylesInspiration" />
                                        <h3>Tooltip Styles Inspiration</h3>
                                    </a>
                                    <a class="dummy-media-object img-radius" href="#!">
                                        <img src="~/Content/assets/images/avatar-3.jpg" class="img-radius" alt="NotificationStyles" />
                                        <h3>Notification Styles Inspiration</h3>
                                    </a>
                                </div>
                            </div>
                            <!-- /morphsearch-content -->
                            <span class="morphsearch-close"><i class="icofont icofont-search-alt-1"></i></span>
                        </div>
                        <!-- search end -->
                    </div>
