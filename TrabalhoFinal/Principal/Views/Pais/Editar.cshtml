
@{
    ViewBag.Title = "Cadastro";
    Layout = "~/Views/Shared/Master.cshtml";
}

<h2>Editar</h2>
<form action="" method="post">
    <input type="hidden" name="pais.Id" id="editar-pais-campo-id" />
    <div class="form-group">
        <label for="cadastro-pais">Nome</label>
        <input type="text" id="editar-campo-pais-nome" class="form-control" name="pais.Nome" />

    </div>
    <div>
        <button type="submit" class="btn btn-sucess float-left right">
            <i class="fa fa-save">Cadastro</i>
        </button>
    </div>
</form>
